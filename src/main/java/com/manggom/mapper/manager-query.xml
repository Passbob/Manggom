<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="selectManagerCode">
        SELECT
         MANAGER_CODE
         FROM MANAGER
    </entry>
    <entry key="inputManager">
        INSERT INTO MANAGER
        (MANAGER_NO, MANAGER_CODE)
        VALUES
        (?, ?);
    </entry>
    <entry key="updateManagerCode">
        UPDATE MANAGER
        SET MANAGER_CODE = ?
        WHERE MANAGER_NO = ?;
    </entry>
</properties>